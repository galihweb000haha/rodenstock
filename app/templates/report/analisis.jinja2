{%extends "layout_dashboard.jinja2" %}
{%block pagestyles %}
    <link rel="stylesheet" href="{{ url_for('static', filename='vendor/css/select2.min.css') }}" class="template-customizer-core-css" />
{% endblock %}
{% block content %}
<div class="container-xxl flex-grow-1 container-p-y">
    <div class="card mt-3">
        <div class="row row-bordered g-0">
            <div class="col-md-12">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Hasil Prediksi Relevansi &#9432;</h5>
                                <div class="chart_hasil_prediksi_relevansi"></div>
                            </div>
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Korelasi Prestasi Terhadap Prediksi &#9432;</h5>
                                <div class="chart_korelasi_prestasi_terhadap_prediksi"></div>
                            </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
        <div class="row row-bordered g-0">
            <div class="col-md-12">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Korelasi Organisasi Terhadap Prediksi &#9432;</h5>
                                <div class="chart_korelasi_organisasi_terhadap_prediksi"></div>
                            </div>
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Korelasi Sertifikat Terhadap Prediksi &#9432;</h5>
                                <div class="chart_korelasi_sertifikat_terhadap_prediksi"></div>
                            </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
        <div class="row row-bordered g-0">
            <div class="col-md-12">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Feature Importance &#9432;</h5>
                                <img src="{{ url_for('static', filename='img/analytics/feature_importance.png') }}" width="100%" />
                            </div>
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Matriks Korelasi &#9432;</h5>
                                <img src="{{ url_for('static', filename='img/analytics/matriks_korelasi.png') }}" width="100%" />
                            </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
        <div class="row row-bordered g-0">
            <div class="col-md-12">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Distribusi Jenis Kelamin &#9432;</h5>
                                <div class="chart_distribusi_jenis_kelamin" ></div>
                            </div>
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Distribusi Pekerjaan Ortu &#9432;</h5>
                                <div class="chart_distribusi_pekerjaan" ></div>
                            </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
        <div class="row row-bordered g-0">
            <div class="col-md-12">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Distribusi Penghasilan Ortu &#9432;</h5>
                                <div class="chart_distribusi_penghasilan_ortu" ></div>
                            </div>
                            <div class="col-md-6">
                                <h5 class="card-header m-0 me-2 pb-3" title="Analisis multivariat adalah metode statistik untuk memahami hubungan antara beberapa variabel sekaligus.">Analisis Deskriptif &#9432;</h5>
                                <div class="analisis_deskripstif" ></div>
                            </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
    </div>
</div>

{% block additionalscripts %}
    <script src="{{ url_for('static', filename='vendor/libs/apex-charts/apexcharts.js') }}"></script>
    <script src="{{ url_for('static', filename='vendor/js/select2.min.js') }}"></script>
    <script src="{{ url_for('static', filename='js/chart.js') }}"></script>

    <script>
        $(document).ready(function() {
            $('.my_selection').select2();
            $('#navbar-collapse .select2-selection__arrow').removeClass('select2-selection__arrow')
            $('#navbar-collapse .select2-selection').css('border', 0)
            $('#search_menu').on('change', function() {
                document.location.href = $(this).val()
            })
        });
    </script>
{% endblock %}
{% endblock %}
